generator client{
    provider = "prisma-client-js"
    previewFeatures = ["driverAdapters"]
}

datasource db{
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model User {
    id       Int   @id @default(autoincrement())
    username String @unique
    password String 
}

model Tanker {
    id       Int   @id @default(autoincrement())
    rute String
    capacity Int
    schedule DateTime
}